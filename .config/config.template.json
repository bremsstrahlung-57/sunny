{
  "api": {
    "key": "your-api-key-goes-here(only openweather api)"
  },
  "defaults": {
    "location": "New York",
    "units": "metric"
  },
  "display": {
    "show_forecast": false,
    "days": 5,
    "col_city": "light_steel_blue",
    "col_temp": {
      "high": "sandy_brown",
      "mid": "chartreuse3",
      "low": "deep_sky_blue1"
    },
    "col_humid": {
      "high": "cornflower_blue",
      "mid": "green_yellow",
      "low": "indian_red"
    },
    "col_desc": {
      "Thunderstorm": "dodger_blue2",
      "Drizzle": "deep_sky_blue2",
      "Rain": "light_steel_blue3",
      "Snow": "bright_white",
      "Atmosphere": "grey53",
      "Clear": "sky_blue1",
      "Clouds": "orchid"
    },
    "col_wind": "dark_orange"
  }
}
